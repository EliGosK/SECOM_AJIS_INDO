    <column id="RemainQty" width="90" type="ron" align="right" format="0,000" sort="na">headerRemainStockOutQty</column>
    <column id="NewInstSale" width="100" type="link" align="right" format="0,000" sort="na">headerDetailColumnHeader</column>
    <column id="NewInstSample" width="100"   type="link" align="right" format="0,000" sort="na">#cspan</column>
    <column id="NewInstRental" width="100" type="link" align="right" format="0,000" sort="na">#cspan</column>
    <column id="SecondhandInstRental" width="100" type="link" align="right" format="0,000" sort="na">#cspan</column>
    <column id="ToJson" width="0" type="ro" align="left" hidden="true"></column>
    
    <afterInit>
      <call command="attachHeader">
        <param>#rspan,#rspan,#rspan,#rspan,#rspan,headerNewInstSale,headerNewInstSample,headerNewInstRental,headerSecondhandInstRental</param>
      </call>
    </afterInit>

    <settings>
      <colwidth>px</colwidth>
    </settings>
  </head>
