    <column id="CollectionArea" width="90" type="ro" align="left" sort="na">lblHeaderCollectionArea</column>
    <column id="CollectionAreaCode" width="0" type="ro" hidden="true"></column>
    <column id="ExpectedCollectDate" width="100" type="ro" align="left" sort="na">lblHeaderExpectedCollectDate</column>
    <column id="Memo" width="100" type="ro" align="left" sort="na">lblMemo</column>
    <column id="Del" width="30" type="ro" align="left" sort="na">  </column>
    <column id="TempSpan" width="0" type="ro" hidden="true"></column>
    <column id="ReceiptAmount" width="0" type="ro" hidden="true"></column>
    <column id="ReceiptAmountCurrencyType" width="0" type="ro" hidden="true"></column>
    <afterInit>
      <call command="attachHeader">
        <param>#rspan,#rspan,#rspan,#rspan,lblHeaderBillingClientName,lblHeaderBillingClientAddress,#rspan,#rspan,#rspan,#rspan,#rspan,#rspan</param>
      </call>
