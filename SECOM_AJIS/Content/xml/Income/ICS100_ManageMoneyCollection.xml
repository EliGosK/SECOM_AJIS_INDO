<?xml version="1.0" encoding="UTF-8"?>
<rows>
  <head>

    <column id="ReceiptNo" width="110" type="ro" align="left" sort="na">lblHeaderReceiptNo</column>
    <column id="ReceiptDate" width="100" type="ro" hidden="true"></column>
    <column id="BillingTargetCode" width="150" type="ro" align="left" sort="na">lblHeaderBillingTargetCode</column>
    <column id="BillingTargetCodeLong" width="0" type="ro" hidden="true"></column>
    <column id="BillingClientName" width="200" type="ro" align="left" sort="na">lblHeaderBillingClient</column>
