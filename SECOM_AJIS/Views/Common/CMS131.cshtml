@using SECOM_AJIS.Common.Helpers
@{
    Layout = "~/Views/Shared/_DialogLayout.cshtml";
    ViewBag.LabelWidth = "150px";
    ViewBag.ObjectWidth = "260px";

    ViewBag.isViewModeFlag = true;
}
@section page_header{

  
    <script type="text/javascript" src="@Url.Content("~/Content/js/dhtmlxgrid/dhtmlxcommon.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/js/dhtmlxgrid/dhtmlxgrid.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/js/dhtmlxgrid/dhtmlxgridcell.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/js/dhtmlxgrid/ext/dhtmlxgrid_data.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/js/dhtmlxgrid/ext/dhtmlxgrid_pgn.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/js/dhtmlxgrid/excells/dhtmlxgrid_excell_dhxcalendar.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/js/dhtmlxgrid/dhtmlxcalendar.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/Common/CMS131.js")" ></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/Base/GridControl.js")"></script>
    
   <script src="../../Scripts/Common/Dialog.js" type="text/javascript"></script>
    
    
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/js/dhtmlxgrid/dhtmlxgrid.css")"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/js/dhtmlxgrid/skins/dhtmlxgrid_dhx_secom.css")"/>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/js/dhtmlxgrid/ext/dhtmlxgrid_pgn_bricks.css")"/>
    
 

    
}
<!-- ==== HTML ====-->

<div style="display:none">
    @Html.Hidden("btnClose", (string)@Resources.CMS131.btnClose)
</div>

<!-- ==== Content ====-->

<div id="divAll_CMS131">

<div class="main-table">
    <div class="usr-row">
        <div class="usr-label" style="width:@ViewBag.LabelWidth;">
            @Resources.CMS131.lblContract_code
        </div>
        <div class="usr-object" style="width:@ViewBag.ObjectWidth;" >
            @Html.CommonTextBox("txtContractCode", (string)@ViewBag.ContractCode, new { @isViewMode = @ViewBag.isViewModeFlag})
        </div>
        <div class="usr-label" style="width:@ViewBag.LabelWidth;">
            @Resources.CMS131.lblOccurrence
        </div>
        <div class="usr-object" style="width:@ViewBag.ObjectWidth;">
            @Html.CommonTextBox("txtOccurrence", (string)@ViewBag.Occurrence, new { @isViewMode = @ViewBag.isViewModeFlag })
        </div>
    </div>
     <div class="usr-row">
        <div class="usr-label" style="width:@ViewBag.LabelWidth;">
            @Resources.CMS131.lblContractOffice
        </div>
        <div class="usr-object" style="width:@ViewBag.ObjectWidth;">
            @Html.CommonTextBox("txtContractOffice", (string)@ViewBag.txtContractOffice, new { @isViewMode = @ViewBag.isViewModeFlag })
        </div>
        <div class="usr-label" style="width:@ViewBag.LabelWidth;">
            @Resources.CMS131.lblOperationOffice
        </div>
        <div class="usr-object" style="width:@ViewBag.ObjectWidth;">
            @Html.CommonTextBox("txtOperationOffice", (string)@ViewBag.txtOperationOffice, new { @isViewMode = @ViewBag.isViewModeFlag })
        </div>
    </div>
     <div class="usr-row">
        <div class="usr-label" style="width:@ViewBag.LabelWidth;">
            @Resources.CMS131.lblNegotiationStaff1
        </div>
        <div class="usr-object" style="width:@ViewBag.ObjectWidth;">
            @Html.CommonTextBox("txtNegotiationStaff1", (string)@ViewBag.txtNegotiationStaff1, new { @isViewMode = @ViewBag.isViewModeFlag})
        </div>
        <div class="usr-label" style="width:@ViewBag.LabelWidth;">
            @Resources.CMS131.lblNegotiationStaff2
        </div>
        <div class="usr-object" style="width:@ViewBag.ObjectWidth;">
            @Html.CommonTextBox("txtNegotiationStaff2", (string)@ViewBag.txtNegotiationStaff2, new { @isViewMode = @ViewBag.isViewModeFlag})
        </div>
    </div>
<div class="usr-clear-style">&nbsp;</div>
</div>

<div class="main-table">
    <div class="table-header">
        @Resources.CMS131.lblSection_ContractDocumentList
    </div>
    
    <div>
        <div id="mygrid_container" style="width: 99%; margin-left: 4px;">
        </div>
    </div>
    <div class="usr-clear-style">&nbsp;</div>
</div>

</div>
<!-- ==== End Content ====-->
