            @Resources.CMS160.lblDelivery_document_receive_date
        </div>
        <div class="usr-object" style="width:@ViewBag.shortText; background-color:@ViewBag.txtBackgroundColor;">
            @Html.CommonTextBox("txtDelivery_document_receive_date", (string)@ViewBag.txtDelivery_document_receive_date, new { isViewMode = @ViewBag.isViewMode })
        </div>
     </div>



     <div class="usr-row">
        <div class="usr-label" style="width:@ViewBag.lbWidth;">
            @Resources.CMS160.lblIE_in_charge
        </div>
        <div class="usr-object" style="width:auto; background-color:@ViewBag.txtBackgroundColor;">
            @Html.CommonTextBox("txtIE_in_charge", (string)@ViewBag.txtIE_in_charge, new { isViewMode = @ViewBag.isViewMode })
        </div>
     </div>

     <div class="usr-row">
        <div class="usr-object" style="height: 100%;">
            <div class="fieldset-table" style="width: 945px; height: 100%; margin-left: 5px ;">
                <div class="fieldset-header">
                    <span>@Resources.CMS160.lblSubcontractor</span>
                </div>

                @*[GRID] SUBCONTRACTOR *@
                <div>
                    <div id="mygrid_subcontractor_Install" style="width: 99%; margin-left: 4px;">
                    </div>
                </div>

                <div class="usr-clear-style">
                    &nbsp;</div>
            </div>
        </div>
    </div>

     <div class="usr-clear-style">&nbsp;</div>
</div>


